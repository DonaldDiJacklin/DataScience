   "source": [
    "X <- cbind(c(rnorm(10)), c(15*rnorm(10)), c(1:10))\n",
    "Phi <-cbind(X,X**2,X**3)\n",
    "Phi <- cbind(rep.int(1,10),Phi)\n",
    "b <- c(12,seq(1.2,10.8, 1.2))\n",
    "b\n",
    "\n",
    "\n",
    "Y <- Phi%*%b + rnorm(10)\n",
    "\n",
    "w<- solve(t(Phi)%*%Phi)%*%t(Phi)%*%Y\n",
    "w\n",
    "\n",
    "R2<- 1-(t(Y-Phi%*%w)%*%(Y-Phi%*%w))/(t(Y - mean(Y))%*%(Y - mean(Y)))\n",
    "R2"
   ]
  }
